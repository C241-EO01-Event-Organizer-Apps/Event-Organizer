<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.eventTenant.DetailEventTenant">

    <TextView
        android:id="@+id/Jdetail_tenant"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:text="Detail Tenant"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@tools:sample/avatars" />

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/black"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/Jdetail_tenant" />


    <TextView
        android:id="@+id/Jnama_tenant"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text="Nama Tenant"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/Jdetail_tenant"
        app:layout_constraintTop_toBottomOf="@+id/divider" />

    <TextView
        android:id="@+id/nama_tenant"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/Jnama_tenant"
        app:layout_constraintTop_toBottomOf="@+id/Jnama_tenant"
        tools:text="Fulan" />
    <TextView
        android:id="@+id/Jkode_tenant"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text="Kode Tenant"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/nama_tenant"
        app:layout_constraintTop_toBottomOf="@+id/nama_tenant" />

    <TextView
        android:id="@+id/kode_tenant"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/Jkode_tenant"
        app:layout_constraintTop_toBottomOf="@+id/Jkode_tenant"
        tools:text="Fulan" />

    <TextView
        android:id="@+id/Jdetail_event"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:text="Detail Event"
        android:textSize="20sp"
        android:layout_marginTop="30dp"
        app:layout_constraintStart_toStartOf="@+id/kode_tenant"
        app:layout_constraintTop_toBottomOf="@+id/kode_tenant"
        tools:src="@tools:sample/avatars" />

    <View
        android:id="@+id/div"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/black"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/Jdetail_event" />


    <TextView
        android:id="@+id/Jnama_event"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text="@string/nama_event"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/Jdetail_event"
        app:layout_constraintTop_toBottomOf="@+id/div" />

    <TextView
        android:id="@+id/nama_event"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/img_item"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/Jnama_event"
        app:layout_constraintTop_toBottomOf="@+id/Jnama_event"
        tools:text="Fulan" />
    <TextView
        android:id="@+id/Jnama_organi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/img_item"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text="Nama Organizer"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="@+id/nama_event"
        app:layout_constraintTop_toBottomOf="@+id/nama_event" />

    <TextView
        android:id="@+id/nama_organi"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="@+id/Jnama_organi"
        app:layout_constraintTop_toBottomOf="@+id/Jnama_organi"
        tools:text="Fulan" />

    <TextView
        android:id="@+id/total"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_goneMarginTop="10dp"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        android:text="total Pengunjung"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="@+id/nama_organi"
        app:layout_constraintTop_toBottomOf="@+id/nama_organi" />


</androidx.constraintlayout.widget.ConstraintLayout>